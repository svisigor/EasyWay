<h1>Listing table_templates</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Cell body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @table_templates.each do |table_template| %>
  <tr>
    <td><%= table_template.name %></td>
    <td><%= table_template.cell_body %></td>
    <td><%= link_to 'Show', table_template %></td>
    <td><%= link_to 'Edit', edit_table_template_path(table_template) %></td>
    <td><%= link_to 'Destroy', table_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Table template', new_table_template_path %>
